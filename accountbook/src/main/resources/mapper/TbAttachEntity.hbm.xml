<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.model.TbAttachEntity" table="tb_attach" schema="dbo" catalog="accountbook">
        <id name="attachId" column="attachID">
            <generator class="uuid2" />
        </id>
        <property name="attachFileName" column="attachFileName"/>
        <property name="attachFileUuid" column="attachFileUUID"/>
        <property name="attachFilePosition" column="attachFilePosition"/>
    </class>
</hibernate-mapping>