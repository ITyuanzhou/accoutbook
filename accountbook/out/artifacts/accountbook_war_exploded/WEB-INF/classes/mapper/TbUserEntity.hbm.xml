<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.zy.model.TbUserEntity" table="tb_user" schema="dbo" catalog="accountbook">
        <id name="uid" column="uid">
            <generator class="uuid2" />
        </id>
        <property name="upwd" column="upwd"/>
        <property name="uname" column="uname"/>
        <property name="upersonBalance" column="upersonBalance"/>
        <property name="uaaBalance" column="uaaBalance"/>
        <property name="ulastLoginTime" column="ulastLoginTime"/>

        <many-to-one name="uattach" class="com.zy.model.TbAttachEntity" column="uavatarAttachID" unique="true"></many-to-one>
    </class>
</hibernate-mapping>